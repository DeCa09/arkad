# .env
RABBITMQ_DEFAULT_USER=admin # Default user for RabbitMQ, needs to be in syn with definitions.json file
RABBITMQ_DEFAULT_PASS=admin123 # Default password for RabbitMQ, needs to be in syn with definitions.json file
EXTRACTION_RESULTS_QUEUE=batch.extraction.results
PROCESSING_RESULTS_QUEUE=batch.processing.results
RABBITMQ_HOST=localhost # Hostname for RabbitMQ, default is localhost (okay for local development)
RABBITMQ_PORT=5672 # Default port. Matches the port in docker-compose.yaml
RABBITMQ_VHOST=/

API_KEY=your_api_key_here # API key for the service, replace with your actual key
DATABASE_URL=postgres://user:password@localhost:5432/mydatabase # Database connection string, replace with your actual database URL